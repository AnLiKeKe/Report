{
  "version": 2,
  "builds": [
    {
        "src": "server.py",
        "use": "@vercel/python",
        "config": {
          "runtime": "python@3.11"
        }
      }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ],
  "env": {
    "PORT": "8000"
  },
  "build": {
    "apt": {
      "packages": ["libpq-dev"]
    }
  }
}